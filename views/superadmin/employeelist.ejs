<div class="content-wrapper" >
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2" >
          <div class="col-sm-6">
            <h1>View Employee</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="<%= baseURL %>/superadmin-login/">Home</a></li>
              <li class="breadcrumb-item active">View Employee </li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>
 <!-- Main content -->
  
    <section class="content">
      <div class="container-fluid"> 


<div class="row">
          <!-- left column-->
          <div class="col-md-24" style="padding:20px;"> 
        
            <!--general form elements -->
            <div class="card card-primary" >
              <div class="card-header" >
                <h3 class="card-title">View User</h3>
              </div>
              <!-- /.card-header -->
               
                <table id="example" class="table table-bordered" >
                 <thead>
                    <th style="width:2px">SNO</th>
                    <th>NAME</th>
                    <th>MOBILE NUMBER</th>
                    <th>FATHER NAME</th>
                    <th>EMAIL ID</th>
                    <th>GENDER</th>
                    <th>DOB</th>
                    <th>MARTIAL STATUS</th>
                    <th>ADDRESS</th>
                    <th>PINCODE</th>
                    <th>CITY</th>
                    <th>STATE</th>
                    <th>PAN CARD</th>
                    <th>AADHAR CARD</th>
                    <th>PASSPORT PHOTO</th>
                    <th>POSITION</th>
                    <th>SALARY</th>
                    <th>JOINING DATE</th>
                    <th>SALARY DATE</th>
                    <th>LOCATION</th>
                  <th style="width: 40px">ACTION</th>
</thead>
<tbody>
    <% employees.forEach(function(employee,index){ %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= employee.name %></td>
                <td><%= employee.mobile %></td>
                <td><%= employee.father_name %></td>
                <td><%= employee.email %></td>
                <td><%= employee.gender %></td>
                <% if(employee.dob){ %>
                <td>
                    <%= employee.dob.toString().substring(0, 10) %>
                </td>
                <% } %>
                <td><%= employee.martial_status %></td>
                <td><%= employee.address %></td>
                <td><%= employee.pincode %></td>
                <td><%= employee.city %></td>
                <td><%= employee.state %></td>
                <td><%= employee.pan_card %></td>
                <td><%= employee.aadhar_card %></td>
                <td>
                 <img src=<%= employee.passport_photo %> width="100px;"  height="150px;">   
                </td>
                
                <td><%= employee.position %></td>
                <td><%= employee.salary %></td>
                <% if(employee.joining_date){ %>
                <td><%= employee.joining_date.toString().substring(0, 10) %></td>
                <% } %>
                <% if(employee.salary_date){ %>
                <td><%= employee.salary_date.toString().substring(0, 10) %></td>
                <% } %>
               <td><%= employee.location %></td>
                <td>
                    <a href="<%= baseURL %>/superadmin-login/edit-employee/<%=employee._id%>" class="btn btn-block btn-primary "><i class="fa-solid fa-pen-to-square"></i></a>
                   <a href="<%= baseURL %>/superadmin-login/employee-delete/<%=employee._id%>" class="btn btn-block btn-danger"><i class="fa-solid fa-trash-can"></i></a>
                </td>
                
            </tr>
    <% }); %>
           

</tbody>

</table> 

<%- include('partials/footer') %>


