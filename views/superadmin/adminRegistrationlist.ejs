<%- include('partials/header') %>

<%- include('partials/navbar') %>

<%- include('partials/sidebar') %>

 


</style>
<div class="content-wrapper" >
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2" >
          <div class="col-sm-6">
            <h1>View user</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="<%= baseURL %>/superadmin-login/">Home</a></li>
              <li class="breadcrumb-item active">View user </li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>
 <!-- Main content -->
  
    <section class="content">
      <div class="container-fluid"> 


<div class="row">
          <!-- left column-->
          <div class="col-md-24" style="padding:10px;">
            <!--general form elements -->
            <div class="card card-primary" >
              <div class="card-header" >
                <h3 class="card-title">View User</h3>
              </div>
              <!-- /.card-header -->
               
                <table id="example" class="table table-bordered" >
                <thead>
                    <th style="width:2px">SNO</th>
                    <th>KYC DOCUMNET</th>
                    <th>AADHAR CARD NUMBER</th>
                    <th>PEN CARD NUMBER</th>
                    <th>FULL NAME</th>
                    <th>DATE OF BIRTH</th>
                    <th>EMAIL</th>
                    <th>PHONE NUMBER</th>
                    <th>REGION</th>
                    <th>BRANCH</th>
                   <th style="width: 40px">ACTION</th>
</thead>
<tbody>
    
    <% adminregistration.forEach(function(adminregistration,index){ %>
            <tr>
                <td><%= index+1 %></td>
                <td>
                <img src=<%= adminregistration.kyc_document %> width="80px;" height="100px;">
                </td>
                <td>
                   <img src=<%= adminregistration.aadhar %> width="80px;"  height="100px;">  
                </td>
                <td>
                    <img src=<%= adminregistration.pan_card %> width="80px;"  height="100px;">   
                </td>
                <td><%= adminregistration.fullname%></td>
               <% if(adminregistration.dob){ %>
                <td><%= adminregistration.dob.toString().substring(0, 10) %></td>
                <% } %>
               <td><%= adminregistration.email%></td>
                <td><%= adminregistration.phone%></td>
                <td><%= adminregistration.region%></td>
                <td><%= adminregistration.branch%></td>
                <td>
                  <a href="https://api.whatsapp.com/send?text={{ You Have Verified }}"
              data-action="share/whatsapp/share" 
 class="btn btn-lg btn-floating btn-success"><i class="fa-brands fa-whatsapp"></i></a>

                </td>
            </tr>
    <% }); %>
          

</tbody>

</table> 

<%- include('partials/footer') %>


